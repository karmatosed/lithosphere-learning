{
	"version": 1,
	"settings": {
		"border": {
			"customColor": true,
			"customRadius": true,
			"customStyle": true,
			"customWidth": true
		},		
		"color": {
			"custom": false,
			"gradients": [],
			"duotone": [
				{
					"colors": [ "#f8e24c", "#939599" ],
					"slug": "yellow-grey",
					"name": "Yellow Grey"
				},
				{
					"colors": [ "#2d2d2b", "#f8e24c" ],
					"slug": "dark-yellow",
					"name": "Dark Yellow"
				},
				{
					"colors": [ "#ffffff", "#f8e24c" ],
					"slug": "light-yellow",
					"name": "Light Yellow"
				}
			],
			"link": false,
			"palette": [
				{
					"slug": "foreground",
					"color": "#1b1b1b",
					"name": "foreground"
				},
				{
					"slug": "background",
					"color": "#ffffff",
					"name": "background"
				},
				{   
					"name": "primary",
					"slug": "primary",
					"color": "#f8e24c"
				},
				{
					"slug": "secondary",
					"color": "#939599",
					"name": "secondary"
				},
				{
					"slug": "mid-grey",
					"color": "#eeeeee",
					"name": "tertiary"
				},
				{
					"slug": "highlight",
					"color": "#fff0e2",
					"name": "highlight"
				},
				{
					"slug": "lowlight",
					"color": "#2b2b2b",
					"name": "lowlight"
				}
			]
		},
		"layout": {
			"contentSize": "720px",
			"wideSize": "1144px"
		},
		"spacing": {
			"customMargin": "true",
			"customPadding": "true",
			"units": [ "px", "em", "rem", "vh", "vw" ]
		},
		"custom": {
			"body": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"lineHeight": 1.7
				}
			},
			"button": {
				"border": {
					"color": "var(--wp--custom--color--primary)",
					"radius": "2px",
					"style": "solid",
					"width": "1px"
				},
				"color": {
					"background": "var(--wp--custom--color--primary)",
					"text": "var(--wp--custom--color--foreground)"
				},
				"hover": {
					"color": {
						"text": "var(--wp--custom--color--background)",
						"background": "var(--wp--custom--color--foreground)"
					},
					"border": {
						"color": "var(--wp--custom--color--secondary)"
					}
				},
				"spacing": {
					"padding": {
						"top": "0.8em",
						"bottom": "0.8em",
						"left": "1.4em",
						"right": "1.4em"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--custom--body--typography--font-family)",
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "normal",
					"lineHeight": 2
				}
			},
			"color": {
				"foreground": "var(--wp--preset--color--foreground)",
				"background": "var(--wp--preset--color--background)",
				"primary":"var(--wp--preset--color--primary)",
				"secondary":"var(--wp--preset--color--secondary)",
				"tertiary":"var(--wp--preset--color--tertiary)",
				"highlight":"var(--wp--preset--color--highlight)",
				"lowlight":"var(--wp--preset--color--lowlight)"
			},
			"fontsToLoadFromGoogle": [
				"family=Inter:wght@100;200;300;400;500;600;700;800;900&display=swap"
			],
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--custom--body--typography--font-family)",
					"fontWeight": 700,
					"lineHeight": 1.3
				}
			},
			"margin": {
				"baseline": "8px",
				"horizontal": "24px",
				"vertical": "24px",
				"vertical-large": "48px"
			},
			"typography": {
				"xs":   "0.8rem",
				"sm":   "1rem",
				"normal": "1.25rem",
				"lg":   "1.563rem",
				"xl":   "1.953rem",
				"xxl":  "2.441rem",
				"xxxl": "3.052rem"
			}
		},
		"typography": {
			"dropCap": true,
			"customFontSize": false,
			"customLineHeight": false,
			"fontSizes": [
				{
					"name": "XS",
					"size": "var(--wp--custom--typography--xs)",
					"slug": "extra small"
				},
				{
					"name": "SM",
					"size": "var(--wp--custom--typography--sm)",
					"slug": "small"
				},
				{
					"name": "Normal",
					"size": "var(--wp--custom--typography--normal)",
					"slug": "normal"
				},
				{
					"name": "LG",
					"size": "var(--wp--custom--typography--lg)",
					"slug": "large"
				},
				{
					"name": "XL",
					"size": "var(--wp--custom--typography--xl)",
					"slug": "extra-large"
				},
				{
					"name": "2XL",
					"size": "var(--wp--custom--typography--xxl)",
					"slug": "huge"
				},
				{
					"name": "3XL",
					"size": "var(--wp--custom--typography--xxxl)",
					"slug": "gigantic"
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "Inter, sans-serif",
					"slug": "inter",
					"name": "Inter"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/code": {
				"color": {
					"background": "var(--wp--custom--color--lowlight)",
					"text": "var(--wp--custom--color--primary)"
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--custom--margin--horizontal)",
						"right": "var(--wp--custom--margin--horizontal)",
						"top": "var(--wp--custom--margin--vertical)",
						"bottom": "var(--wp--custom--margin--vertical)"
					}
				},
				"typography": {
					"fontFamiliy": "monospace"
				}
			},
			"core/post-date": {
				"color": {
					"link": "var(--wp--custom--color--foreground)",
					"text": "var(--wp--custom--color--foreground)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/rss":{
				"color":{
					"text": "#950E7F"
				},
				"typography": {
					"fontSize": "1.563rem"
				}
			},
			"core/image":{
				"color":{
					"background": "currentColor"
				},
				"spacing": {
					"margin":{
						"bottom": "var(--wp--custom--margin--vertical-large)",
						"top": "var(--wp--custom--margin--vertical)"
					},
					"padding": {
						"bottom": "1px",
						"top": "1px",
						"right": "1px",
						"left": "1px"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"core/quote": {
				"border": {
					"color": "var(--wp--custom--color--foreground)",
					"style": "solid",
					"width": "0 0 0 1px"
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--custom--margin--horizontal)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontStyle": "normal"
				}
			},
			"core/separator": {
				"color": {
					"text": "var(--wp--custom--color--teriary)"
				},
				"border": {
					"color": "currentColor",
					"style": "solid",
					"width": "0 0 1px 0"
				}
			}
		},
		"color": {
			"background": "var(--wp--custom--color--background)",
			"text": "var(--wp--custom--color--foreground)"
		},
		"elements": {
			"h1": {
				"spacing":{
					"margin":{
						"bottom": "var(--wp--custom--margin--vertical)",
						"top": "var(--wp--custom--margin--vertical)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--custom--heading--typography--font-family)",
					"fontSize": "var(--wp--preset--font-size--huge)",
					"lineHeight": 1.2
				}
			},
			"h2": {
				"spacing":{
					"padding":{
						"bottom": "var(--wp--custom--margin--vertical)/2",
						"top": "var(--wp--custom--margin--vertical)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--custom--heading--typography--font-family)",
					"fontSize": "var(--wp--preset--font-size--extra-large)",
					"fontWeight": "var(--wp--custom--heading--typography--font-weight)",
					"lineHeight": 1.2
				}
			},
			"h3": {
				"spacing":{
					"padding":{
						"bottom": "var(--wp--custom--margin--vertical)/2",
						"top": "var(--wp--custom--margin--vertical)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--custom--heading--typography--font-family)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--heading--typography--font-weight)",
					"lineHeight": "var(--wp--custom--heading--typography--line-height)"
				}
			},
			"h4": {
				"spacing":{
					"padding":{
						"bottom": "var(--wp--custom--margin--vertical)/2",
						"top": "var(--wp--custom--margin--vertical)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--custom--heading--typography--font-family)",
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "var(--wp--custom--heading--typography--font-weight)",
					"lineHeight": "var(--wp--custom--heading--typography--line-height)"
				}
			},
			"h5": {
				"spacing":{
					"padding":{
						"bottom": "var(--wp--custom--margin--vertical)/2",
						"top": "var(--wp--custom--margin--vertical)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--custom--heading--typography--font-family)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--heading--typography--font-weight)",
					"lineHeight": "var(--wp--custom--heading--typography--line-height)"
				}
			},
			"h6": {
				"spacing":{
					"padding":{
						"bottom": "var(--wp--custom--margin--vertical)/2",
						"top": "var(--wp--custom--margin--vertical)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--custom--heading--typography--font-family)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--heading--typography--font-weight)",
					"lineHeight": "var(--wp--custom--heading--typography--line-height)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--custom--color--foreground)"
				}
			}
		},
		"typography": {
			"lineHeight": "var(--wp--custom--body--typography--line-height)",
			"fontFamily": "var(--wp--custom--body--typography--font-family)",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"fontWeight": "400"
		}
	}
}